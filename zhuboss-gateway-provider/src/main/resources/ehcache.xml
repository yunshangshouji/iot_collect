<?xml version="1.0" encoding="UTF-8"?> 
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:noNamespaceSchemaLocation="../bin/ehcache.xsd"
    updateCheck="false"> 
    <defaultCache overflowToDisk="false" eternal="false" maxElementsInMemory="10000000"/> 
    <!-- <diskStore path="c:/cache" />  -->
    
    <cache name="COLLECTOR_INFO" maxElementsInMemory="100000" timeToLiveSeconds="10" />

    <cache name="CycleSeconds" maxElementsInMemory="10000" timeToLiveSeconds="300" />

    <cache name="CollectorPO" maxElementsInMemory="1000" timeToLiveSeconds="60" />

</ehcache>
